{
  "expo": {
    "name": "lecourteau-client",
    "slug": "lecourteau-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-yellow.png",
      "resizeMode": "cover",
      "backgroundColor": "#F7A600"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hedilarbi95.lecourteauclient",

      "config": {
        "googleMapsApiKey": "AIzaSyC2t8GvZFa6Ld6fbKM6_m2n3M0JoOmI03w"
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.285470002492-rv13ncgqg5qvndr2lqj74hs4nerah1kj"
            ]
          }
        ]
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#F7A600",
      "translucent": false
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",

      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC2t8GvZFa6Ld6fbKM6_m2n3M0JoOmI03w"
        }
      },
      "package": "com.hedilarbi95.lecourteauclient",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cfd95bcc-7f5d-4868-b417-28aee71e3a2f"
      }
    },
    "plugins": [
      ["expo-localization"],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#000000",
          "sounds": ["./assets/sounds/notificationsound.wav"]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permettre $(PRODUCT_NAME) d'accèder à votre emplacement"
        }
      ]
    ]
  }
}
